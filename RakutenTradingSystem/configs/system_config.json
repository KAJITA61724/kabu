{
  "data_collection": {
    "interval_minutes": 5,
    "symbols_file": "prime_symbols.csv",
    "min_volume_threshold": 300000,
    "market_hours": {
      "start": "09:00",
      "end": "15:00"
    },
    "database_path": "enhanced_trading.db"
  },
  "demo_trading": {
    "capital": 2000000,
    "max_positions": 3,
    "position_size_ratio": 0.25,
    "max_holding_days": 1,
    "profit_target": 0.02,
    "stop_loss": 0.008,
    "database_path": "enhanced_demo_trading.db"
  },
  "real_trading": {
    "capital": 2000000,
    "max_positions": 3,
    "position_size_ratio": 0.25,
    "max_holding_minutes": 40,
    "check_interval_seconds": 300,
    "ml_confidence_threshold": 0.8,
    "strategy_confidence_threshold": 0.7
  },
  "backtest": {
    "default_period_days": 30,
    "min_trades_for_analysis": 10,
    "report_output_dir": "reports/backtest"
  },
  "logging": {
    "level": "INFO",
    "log_dir": "logs",
    "max_log_files": 30,
    "monthly_rotation": true
  },
  "reports": {
    "base_dir": "reports",
    "subdirs": {
      "backtest": "reports/backtest",
      "demo": "reports/demo", 
      "quality": "reports/quality",
      "trading": "reports/trading"
    },
    "formats": ["json", "txt", "png"],
    "retention_days": 90
  }
}
